{
  "name": "website",
  "version": "0.0.0",
  "description": "My personal portfolio website",
  "author": "Hezekiah Hendry <hezekiah.hendry@gmail.com>",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rimraf dist node_modules/.cache",
    "lint": "eslint src --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "fetch-docs": "node scripts/fetch-docs.mjs",
    "start": "vite",
    "build": "yarn fetch-docs && vite build",
    "update": "yarn upgrade-interactive",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@sapphire/fetch": "^2.4.1",
    "@solid-primitives/media": "^2.2.3",
    "@solid-primitives/storage": "^1.3.11",
    "@solidjs/meta": "^0.28.5",
    "@solidjs/router": "^0.8.2",
    "highlight.js": "^11.8.0",
    "solid-icons": "^1.0.11",
    "solid-js": "^1.7.7",
    "solid-markdown": "^1.2.2",
    "solid-transition-group": "^0.2.2",
    "typedoc-json-parser": "^8.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@sapphire/eslint-config": "^4.4.3",
    "@sapphire/ts-config": "^4.0.0",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^20.4.1",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "markdown-it": "^13.0.1",
    "postcss": "^8.4.25",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "rimraf": "^5.0.1",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.2",
    "vite-plugin-solid": "^2.7.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RealShadowNova/website.git"
  },
  "bugs": {
    "url": "https://github.com/RealShadowNova/website/issues"
  },
  "homepage": "https://github.com/RealShadowNova/website",
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix --ext ts,tsx"
  },
  "packageManager": "yarn@3.6.1"
}
